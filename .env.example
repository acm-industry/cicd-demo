# CI/CD Authentication Tokens
# Copy this file to .env and fill in your tokens
# IMPORTANT: .env is gitignored and will NOT be committed

# Vercel Authentication
# Get token from: https://vercel.com/account/tokens
VERCEL_TOKEN=

# Vercel Project IDs (get from: vercel link in frontend directory)
VERCEL_ORG_ID=
VERCEL_PROJECT_ID=

# Render Authentication
# Get API key from: https://dashboard.render.com/u/settings#api-keys
RENDER_API_KEY=

# Note: These tokens grant full access to your accounts
# Keep them secure and never commit the .env file to git
