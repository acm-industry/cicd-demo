{
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "outputDirectory": ".next",
  "env": {
    "NEXT_PUBLIC_API_URL": "https://cicd-demo-backend.onrender.com"
  },
  "build": {
    "env": {
      "NEXT_PUBLIC_API_URL": "@api_url"
    }
  },
  "git": {
    "deploymentEnabled": true
  },
  "regions": ["iad1"],
  "github": {
    "silent": false,
    "autoAlias": true
  }
}
